#include<stdio.h>
#include<conio.h>

int main()
{
    int n;
    printf("Enter the year n:");
    scanf("%d",&n);
    if(n%100==0)
    {
        if(n%400==0)
        {
            printf("It is a leap year");
        }
        else
        {
            printf("It is not a leap year");
        }

    }
    return 0;
}